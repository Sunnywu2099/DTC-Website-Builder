<div class="index-page-fifth" node-name="fifthPage">
  <div class="index-page-slide-box">
    <div class="index-page-slide-backdrop">
      <div class="product-text">
        <span class="color">{{ section.settings.title1 }}</span>
      </div>
      <div class="product-title">{{ section.settings.title2 }}</div>
      <div class="station-menus" node-name="fifthMenus">
        {% for block in section.blocks %}
            <div node-name="fifthMenu" data-key="{{ forloop.index0 }}" class="station-menu selected">
                {{ block.settings.menu }}
                {% if block.settings.menu_sub != blank %}
                    <span class="sub" data-key="{{ forloop.index0 }}">{{ block.settings.menu_sub }}</span>
                {% endif %}
            </div>
        {% endfor %}
      </div>
    </div>
    <div node-name="fifthSlide" class="index-page-slide-1 show">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu1_desc }}
        </div>
        <div class="fifth-rows">
          <div class="product-question">{{ section.settings.menu1_remark1 }}</div>
          <a href="{{ section.settings.menu1_btn_link }}" class="buy-now">
            {{- section.settings.menu1_btn -}}
            <i class="icon"></i
          ></a>
        </div>
        <div class="product-remark">{{ section.settings.menu1_remark2 }}</div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu1_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu1_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-2">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu2_desc }}
        </div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu2_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu2_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-3">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu3_desc }}
        </div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu3_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu3_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-4">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu4_desc }}
        </div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu4_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu4_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-5">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu5_desc }}
        </div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu5_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu5_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-6">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu6_desc }}
        </div>
      </div>
      <img
        loading="lazy"
        class="product-img pc-done"
        src="{{ section.settings.menu6_pc_backdrop_url | img_url: '1920x' }}"
        alt=""
      >
      <img
        loading="lazy"
        class="product-img mobile-done"
        src="{{ section.settings.menu6_mb_backdrop_url | img_url: '750x' }}"
        alt=""
      >
    </div>
    <div node-name="fifthSlide" class="index-page-slide-7">
      <div class="index-page-content">
        <div class="product-desc">
          {{ section.settings.menu7_desc }}
        </div>
      </div>
      <div class="product-video product-img">
        <div class="video-warp">
          <video
            node-name="pcVideo"
            class="screen-video pc-done"
            src="{{ section.settings.menu7_pc_video_url }}"
            muted
            loop
            playsinline
            webkit-playsinline
            preload="metadata"
          ></video>
          <video
            node-name="mobileVideo"
            class="screen-video mobile-done"
            src="{{ section.settings.menu7_mb_video_url }}"
            muted
            loop
            playsinline
            webkit-playsinline
            preload="metadata"
          ></video>
        </div>
      </div>
    </div>
  </div>
</div>

{% schema %}
{
  "name": "Index page fifth",
  "class": "shopify-new-index",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "id": "title1",
      "label": "标题一",
      "default": "Automatic Base Station"
    },
    {
      "type": "text",
      "id": "title2",
      "label": "标题二",
      "default": "One Station to Take Care of All"
    },
    
    {
      "type": "text",
      "id": "menu2",
      "label": "第二屏名称",
      "default": "Auto Mop Washing"
    },
    {
      "type": "richtext",
      "id": "menu2_desc",
      "label": "第二屏简介",
