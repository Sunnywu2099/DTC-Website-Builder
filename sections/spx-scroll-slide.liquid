    {
      "type": "richtext",
      "id": "menu6_desc",
      "label": "第六屏简介",
      "default": "<p> Cleans everything, even itself.</p>"
    },

    {
      "label": "第六屏PC背景图",
      "type": "image_picker",
      "info": "请上传5M以内",
      "id": "menu6_pc_backdrop_url"
    },
    {
      "label": "第六屏H5背景图",
      "type": "image_picker",
      "info": "请上传5M以内",
