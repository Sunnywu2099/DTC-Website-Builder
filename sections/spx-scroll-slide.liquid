    {
      "type": "richtext",
      "id": "menu5_desc",
      "label": "第五屏简介",
      "default": "<p>Automatically refills Freo’s water tank. Precise solution control ensures the optimal clean.</p>"
    },
    {
      "label": "第五屏PC背景图",
      "type": "image_picker",
      "info": "请上传5M以内",
      "id": "menu5_pc_backdrop_url"
    },
    {
      "label": "第五屏H5背景图",
      "type": "image_picker",
      "info": "请上传5M以内",
      "id": "menu5_mb_backdrop_url"
    },
    {
