      "id": "menu2_pc_backdrop_url"
    },
    {
      "label": "第二屏H5背景图",
      "type": "image_picker",
      "info": "请上传5M以内",
      "id": "menu2_mb_backdrop_url"
    },
    {
      "type": "text",
      "id": "menu3",
      "label": "第三屏名称",
