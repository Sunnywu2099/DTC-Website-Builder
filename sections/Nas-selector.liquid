

{% assign obj = page.metafields.custom.selector.value | parse_json %}

{% assign bays = obj.bays.value %}
{% assign functions = obj.functions.value %}
{% assign number = obj.number.value %}
{% assign products = obj.products.value %}
{% assign userType = obj.user_type.value %}

<div class="nas-selector">
  <!-- Example of displaying JSON data -->
  <pre>{{ obj | json }}</pre>
  <pre>{{ bays | json }}</pre>
  <pre>{{ functions | json }}</pre>
  <pre>{{ number | json }}</pre>
  <pre>{{ products | json }}</pre>
  <pre>{{ userType | json }}</pre>
</div>


<script type="module">
</script>

{% schema %}
  {
    "name": "Nas selector",
    "settings": [
    ],
    "presets": [
      {
        "name": "Nas selector"
      }
    ]
  }
{% endschema %}